{
  "name": "bradandadrianne.com",
  "version": "1.0.0",
  "description": "Brad & Adrianne's Wedding Website",
  "scripts": {
    "dist": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --open --config webpack.dev.js --port 8081",
    "lint": "eslint src --ext .js,.jsx",
    "lint-fix": "eslint src --fix --ext .js,.jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brodrigu/bradandadrianne.com.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test"
    }
  },
  "jest": {
    "collectCoverage": true,
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testURL": "file:/"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brodrigu/bradandadrianne.com/issues"
  },
  "homepage": "https://bradandadrianne.com",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "jest": "^24.9.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "lodash.debounce": "^4.0.8",
    "material-ui-superselectfield": "^1.10.0",
    "mdi-material-ui": "^6.2.0",
    "moment": "^2.29.4",
    "prop-types": "^15.6.2",
    "randomcolor": "^0.5.3",
    "react": "^16.9.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.4.1",
    "react-google-maps": "^9.4.5",
    "react-modal": "^3.6.1",
    "react-photo-gallery": "^8.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  }
