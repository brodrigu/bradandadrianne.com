{
    "presets" : [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/transform-runtime"],
      ["@babel/plugin-proposal-class-properties"],
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "~app": "./src",
          "~components": "./src/components",
          "~ducks": "./src/ducks",
          "~services": "./src/services"
        }
      }]
    ]
  }
  